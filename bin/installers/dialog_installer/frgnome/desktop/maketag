#!/bin/sh
#item   ####description                                      ###on off ###
TMP=/var/log/setup/tmp
if [ ! -d $TMP ]; then
  mkdir -p $TMP
fi
cat /dev/null > $TMP/SeTnewtag
dialog --title "SELECTING PACKAGES FROM SERIES GNOME Desktop Requirements" \
--checklist "Please select the packages you wish to install \
from the GNOME Desktop Requirements series. This set contains optional packages \
needed for the GNOME Desktop Environment. \\n\\n\
" 22 70 8 \
"alacarte" "" "on" \
"bug-buddy" "" "on" \
"control-center" "" "on" \
"dasher" "" "on" \
"eel" "" "on" \
"ekiga" "" "on" \
"eog" "" "on" \
"epiphany" "" "on" \
"epiphany-extensions" "" "on" \
"evince" "" "on" \
"evolution" "" "on" \
"evolution-caldav" "" "on" \
"evolution-data-server" "" "on" \
"evolution-exchange" "" "on" \
"evolution-webcal" "" "on" \
"file-roller" "" "on" \
"frgnome-desktop" "" "on" \
"gal2" "" "on" \
"gcalctool" "" "on" \
"gconf-editor" "" "on" \
"gdm" "" "on" \
"gedit" "" "on" \
"gnome-applets" "" "on" \
"gnome-backgrounds" "" "on" \
"gnome-bluetooth" "" "on" \
"gnome-desktop" "" "on" \
"gnome-doc-utils" "" "on" \
"gnome-games" "" "on" \
"gnome-icon-theme" "" "on" \
"gnome-keyring-manager" "" "on" \
"gnome-mag" "" "on" \
"gnome-media" "" "on" \
"gnome-menus" "" "on" \
"gnome-netstatus" "" "on" \
"gnome-nettool" "" "on" \
"gnome-panel" "" "on" \
"gnome-pilot" "" "on" \
"gnome-pilot-conduits" "" "on" \
"gnome-session" "" "on" \
"gnome-speech" "" "on" \
"gnome-spell" "" "on" \
"gnome-system-monitor" "" "on" \
"gnome-system-tools" "" "on" \
"gnome-terminal" "" "on" \
"gnome-themes" "" "on" \
"gnome-user-docs" "" "on" \
"gnome-user-share" "" "on" \
"gnome-utils" "" "on" \
"gnome-volume-manager" "" "on" \
"gnopernicus" "" "on" \
"gob2" "" "on" \
"gok" "" "on" \
"gst-plugins-bad" "" "on" \
"gst-plugins-base" "" "on" \
"gst-plugins-good" "" "on" \
"gst-plugins-ugly" "" "on" \
"gstreamer10" "" "on" \
"gtk-engines" "" "on" \
"gtkhtml" "" "on" \
"gtksourceview" "" "on" \
"gucharmap" "" "on" \
"libbtctl" "" "on" \
"libcroco" "" "on" \
"libgail-gnome" "" "on" \
"libgnomeprintui" "" "on" \
"libgnomesu" "" "on" \
"libgsf" "" "on" \
"libgtkhtml" "" "on" \
"libgtop" "" "on" \
"librsvg" "" "on" \
"libsoup" "" "on" \
"libwnck" "" "on" \
"libxklavier" "" "on" \
"metacity" "" "on" \
"nautilus" "" "on" \
"nautilus-cd-burner" "" "on" \
"nautilus-sendto" "" "on" \
"scrollkeeper" "" "on" \
"sound-juicer" "" "on" \
"startup-notification" "" "on" \
"system-tools-backends" "" "on" \
"totem" "" "on" \
"vino" "" "on" \
"vte" "" "on" \
"xscreensaver" "" "on" \
"yelp" "" "on" \
"zenity" "" "on" \
2> $TMP/SeTpkgs
if [ $? = 1 -o $? = 255 ]; then
  rm -f $TMP/SeTpkgs
  cat /dev/null > $TMP/SeTnewtag
  for PKG in alacarte bug-buddy control-center dasher eel ekiga eog epiphany epiphany-extensions evince evolution evolution-caldav evolution-data-server evolution-exchange evolution-webcal file-roller frgnome-desktop gal2 gcalctool gconf-editor gdm gedit gnome-applets gnome-backgrounds gnome-bluetooth gnome-desktop gnome-doc-utils gnome-games gnome-icon-theme gnome-keyring-manager gnome-mag gnome-media gnome-menus gnome-netstatus gnome-nettool gnome-panel gnome-pilot gnome-pilot-conduits gnome-session gnome-speech gnome-spell gnome-system-monitor gnome-system-tools gnome-terminal gnome-themes gnome-user-docs gnome-user-share gnome-utils gnome-volume-manager gnopernicus gob2 gok gst-plugins-bad gst-plugins-base gst-plugins-good gst-plugins-ugly gstreamer10 gtk-engines gtkhtml gtksourceview gucharmap libbtctl libcroco libgail-gnome libgnomeprintui libgnomesu libgsf libgtkhtml libgtop librsvg libsoup libwnck libxklavier metacity nautilus nautilus-cd-burner nautilus-sendto scrollkeeper sound-juicer startup-notification system-tools-backends totem vino vte xscreensaver yelp zenity ; do
    echo "$PKG: SKP" >> $TMP/SeTnewtag
  done
  exit
fi
cat /dev/null > $TMP/SeTnewtag
for PKG in alacarte bug-buddy control-center dasher eel ekiga eog epiphany epiphany-extensions evince evolution evolution-caldav evolution-data-server evolution-exchange evolution-webcal file-roller frgnome-desktop gal2 gcalctool gconf-editor gdm gedit gnome-applets gnome-backgrounds gnome-bluetooth gnome-desktop gnome-doc-utils gnome-games gnome-icon-theme gnome-keyring-manager gnome-mag gnome-media gnome-menus gnome-netstatus gnome-nettool gnome-panel gnome-pilot gnome-pilot-conduits gnome-session gnome-speech gnome-spell gnome-system-monitor gnome-system-tools gnome-terminal gnome-themes gnome-user-docs gnome-user-share gnome-utils gnome-volume-manager gnopernicus gob2 gok gst-plugins-bad gst-plugins-base gst-plugins-good gst-plugins-ugly gstreamer10 gtk-engines gtkhtml gtksourceview gucharmap libbtctl libcroco libgail-gnome libgnomeprintui libgnomesu libgsf libgtkhtml libgtop librsvg libsoup libwnck libxklavier metacity nautilus nautilus-cd-burner nautilus-sendto scrollkeeper sound-juicer startup-notification system-tools-backends totem vino vte xscreensaver yelp zenity ; do
  if grep \"$PKG\" $TMP/SeTpkgs 1> /dev/null 2> /dev/null ; then
    echo "$PKG: ADD" >> $TMP/SeTnewtag
  else
    echo "$PKG: SKP" >> $TMP/SeTnewtag
  fi
done
rm -f $TMP/SeTpkgs
