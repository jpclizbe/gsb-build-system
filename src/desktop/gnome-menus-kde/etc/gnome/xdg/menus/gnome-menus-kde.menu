        <Filename>kde4-koshell.desktop</Filename>
        <Filename>Kontact.desktop</Filename>
        <Filename>kde-Kontact.desktop</Filename>
        <Filename>kde4-Kontact.desktop</Filename>
        <Filename>kword.desktop</Filename>
        <Filename>kde-kword.desktop</Filename>
        <Filename>kde4-kword.desktop</Filename>
        <Filename>kspread.desktop</Filename>
        <Filename>kde-kspread.desktop</Filename>
