<Menu>
    <Name>KDE Applications</Name>
    <Directory>KDE.directory</Directory>
    <Include>
      <And>
        <Category>KDE</Category>
        <Category>Core</Category>
        <Not><Filename>mepis-system-oscenter.desktop</Filename></Not>
      </And>
    </Include>
    <!-- KDE Development -->
    <Menu>
      <Name>Development</Name>
      <Directory>kde-development.directory</Directory>
      <Include>
        <And>
          <Or>
            <Category>KDE</Category>
            <Category>Qt</Category>
          </Or>
          <Category>Development</Category>
          <Not><Or>
            <Category>X-KDE-KDevelopIDE</Category>
            <Category>Translation</Category>
            <Category>WebDevelopment</Category>
          </Or></Not>
        </And>
        <Filename>assistant.desktop</Filename>
        <Filename>designer.desktop</Filename>
        <Filename>linguist.desktop</Filename>
      </Include>
      <Exclude>
        <!-- Exclude KDevelop -->
        <Filename>Development-kdevelop3.desktop</Filename>
        <Filename>kde-kdevassistant.desktop</Filename>
        <Filename>kde-kdevdesigner.desktop</Filename>
        <Filename>kde-kdevelop.desktop</Filename>
        <Filename>kde-kdevelop3.desktop</Filename>
        <Filename>kde-kdevelop_c_cpp.desktop</Filename>
        <Filename>kde-kdevelop_kde_cpp.desktop</Filename>
        <Filename>kde-kdevelop_ruby.desktop</Filename>
        <Filename>kde-kdevelop_scripting.desktop</Filename>
        <!-- Exclude Translation -->
        <Filename>kde-catalogmanager.desktop</Filename>
        <Filename>kde-kbabel.desktop</Filename>
        <Filename>kde-kbabeldict.desktop</Filename>
        <!-- Exclude Web Development -->
        <Filename>kde-kimagemapeditor.desktop</Filename>
        <Filename>kde-klinkstatus.desktop</Filename>
        <Filename>kde-kxsldbg.desktop</Filename>
        <Filename>kde-quanta.desktop</Filename>
      </Exclude>
      <!-- KDE KDevelop -->
      <Menu>
        <Name>KDevelop</Name>
        <Directory>kde-development-kdevelop.directory</Directory>
        <Include>
          <Category>X-KDE-KDevelopIDE</Category>
          <Filename>Development-kdevelop3.desktop</Filename>
          <Filename>kde-kdevassistant.desktop</Filename>
          <Filename>kde-kdevdesigner.desktop</Filename>
          <Filename>kde-kdevelop.desktop</Filename>
          <Filename>kde-kdevelop3.desktop</Filename>
          <Filename>kde-kdevelop_c_cpp.desktop</Filename>
          <Filename>kde-kdevelop_kde_cpp.desktop</Filename>
          <Filename>kde-kdevelop_ruby.desktop</Filename>
          <Filename>kde-kdevelop_scripting.desktop</Filename>
        </Include>
      </Menu>   <!-- End KDevelop -->
      <!-- KDE Translation -->
      <Menu>
        <Name>Translation</Name>
        <Directory>kde-development-translation.directory</Directory>
        <Include>
          <And>
            <Or>
              <Category>KDE</Category>
              <Category>Qt</Category>
            </Or>
            <Category>Translation</Category>
          </And>
          <Filename>kde-catalogmanager.desktop</Filename>
          <Filename>kde-kbabel.desktop</Filename>
          <Filename>kde-kbabeldict.desktop</Filename>
        </Include>
      </Menu>   <!-- End Translation -->
      <!-- KDE Web Development -->
      <Menu>
        <Name>Web Development</Name>
        <Directory>kde-development-webdevelopment.directory</Directory>
        <Include>
          <And>
            <Or>
              <Category>KDE</Category>
              <Category>Qt</Category>
            </Or>
            <Category>WebDevelopment</Category>
          </And>
          <Filename>kde-kimagemapeditor.desktop</Filename>
          <Filename>kde-klinkstatus.desktop</Filename>
          <Filename>kde-kxsldbg.desktop</Filename>
          <Filename>kde-quanta.desktop</Filename>
        </Include>
      </Menu>   <!-- End Web Development -->
    </Menu>   <!-- End KDE Development -->
