+sub is_old_patch_version()
+{
+    return 0 if $^O =~ /cygwin/i;
+
+    my $Patch;
+    my $ver_line;
+    my $is_old = 1;
+
+    open ($Patch, "patch --version|") || die "Can't run patch: $!";
+    $ver_line = <$Patch>;
+    $ver_line =~ m/\s+(\d+)\.(\d+)\.(\d+)/ || die "Can't get patch version\n";
+    if ($1 >= 2 && $2 >= 5 && $3 >= 9) {
+	$is_old = 0;
+    }
+    
+    if ($is_old) {
+	print "Old patch version - pruning LFs\n";
+    }
+    return $is_old;
+}
