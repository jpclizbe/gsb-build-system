+sub scan_unused($$)
+{
+    my $dir = shift;
+    my $patches = shift;
+    my $dirh;
+    my $warned = 0;
+
+    opendir($dirh, $dir) || die "Can't open $dir: $!";
+    while (my $file = readdir ($dirh)) {
+	$file =~ /^\./ && next;     # hidden
+	$file =~ /\.diff$/ || next; # non-patch
+
+	if (!defined $patches->{$file}) {
