+sub count_patch_lines ($$$$)
+{
+    my ($patch, $plus, $minus, $plines) = @_;
+    my @lines = split (/\n/, slurp (find_patch_file ($patch)));
+
+    foreach $line (@lines) {
+	$line =~ /^\+[^\+]/ && $$plus++;
+	$line =~ /^\-[^\-]/ && $$minus++;
+	$$plines++;
+    }
+}
