+sub print_line_count ($)
+{
+    my $patches = shift;
+    my %breakdown;
+
+    for my $patch (keys %{$patches}) {
+	my $set = find_patch_file ($patch);
+	$set =~ s|.*/(.*)/.*|$1|;
+#	print "Path '$set'\n";
+	if (!defined $breakdown{$set}) {
+	    $breakdown{$set} = 0;
+	}
+	$breakdown{$set}++;
+    }
+    for my $set (sort keys %breakdown) {
+	print "$set\t";
+    }
+    print "\n";
+    for my $set (sort keys %breakdown) {
+	print "$breakdown{$set}\t";
+    }
+    print "\n";
+}
